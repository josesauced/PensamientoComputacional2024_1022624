#Imprime si el n첫ero es primo 
def es_primo (numero):
    if numero <=1: 
        return False 
    divisor = 2 
     while divisor * divisor <= numero:
       if numero % divisor ==0:
          return False 
       divisor +=1
    return True
numero = int (input("ingrese un numero para verificar si es primo:"))
if es_primo(numero): 
   print (f"{numero} es un nuemro primo.")
else:
    print (f"{numero} no es un nuemro primo.")

